@page
@model IndexModel
@{
    ViewData["Title"] = "EventPlanner";
}
<html>
	<head>
        <title>Event Planner</title>
        <link rel="stylesheet" href="site.css" />
        <!--Burger icon-->
    <link href="data:image/x-icon;base64," rel="icon" type="image/x-icon">
	</head>

<body>
    <!-- Alert at top of screen-->
    <div class="alert alert-warning textAlign">
        <strong>Warning!</strong> Site is still under construction.
    </div>
    
    <!--Main text-->
    <h1 class="textFont title">Welcome to the Event Planner Web App!</h1>
    <h2 class="h2">Please Search for a Location Below</h2>
  
    <!--Inline Javascript button-->
        <button type="button"
            onclick="document.getElementById('dateTime').innerHTML = Date()">
            Current Date/Time
        </button>
        <p id="dateTime"></p>

</body>
</html>
<!--Bootstrap button-->
<a href="/SearchResults" button class="button button1">Search Results</a>